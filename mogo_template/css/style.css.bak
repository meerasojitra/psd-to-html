@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700;800;900&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Kaushan+Script&display=swap');
*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
body {
	color: #999;
	font-size: 13px;
	font-weight: 400;
	overflow-x: hidden;
	position: relative;
	font-family: 'Roboto', sans-serif;
}
a {
	text-decoration: none;
	outline: none;
	color: inherit;
	-webkit-transition: all .3s ease-in-out;
	   -moz-transition: all .3s ease-in-out;
		 -o-transition: all .3s ease-in-out;
		-ms-transition: all .3s ease-in-out;
			transition: all .3s ease-in-out;
}
a:hover{
    color: inherit;
}
.navbar{
    color: #fff!important;
    font-family: 'Montserrat', sans-serif;
}
.navbar .navbar-brand{
    font-weight: 700;
    font-size: 30px;
    padding: 15px 15px 15px 0px;
    line-height: 20px;  
}
.navbar .navbar-nav .nav-item .nav-link{
    font-weight: 500;
    text-transform: uppercase;
    border-bottom: 3px solid transparent;
    padding: 15px 5px 5px 0px;
    margin: 0 12px;
}
.nav-icon-group li{
    position: relative;
    display: block;
}
.nav-icon-group li a {
    font-weight: 500;
    text-transform: uppercase;
    border-bottom: 3px solid transparent;
    padding: 15px 5px 5px 0px;
    margin: 0px 12px;
    font-size: 18px;
}
.navbar-toggler {
    position: relative;
    padding: 9px 10px;
    margin-top: 8px;
    margin-right: 15px;
    margin-bottom: 8px;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #fff;
}
.navbar-toggler-icon{
    background-image:url(../img/bars.svg);
    color:#fff;
}
.navbar-toggler-icon{
    width: 25px;
    height: 25px;
}
.home-banner{
    padding: 0px;
    top: 0px;
    z-index: 1;
    background: url(../img/home-banner.png);
    background-size: cover;
    height: 100vh;
    position: relative;
    width: 100%;
}
.banner-inner {
    position: absolute;
    overflow: hidden;
    width: 100%;
    top: 40%;
  /* padding: 170px 0 0 0; */
}
.main-heading{
    text-align: center;
}
.main-heading h2{
    font-weight: 700;
    line-height: 1;
    position: relative;
    padding-bottom: 20px;
    margin-bottom: 30px;
    font-size: 110px;
    color: #fff;
    text-transform: uppercase;
}
.main-heading h2::after{
    content: "";
    position: absolute;
    left: 50%;
    bottom: 0;
    width: 50px;
    height: 2px;
    background: #fff;
    transform: translateX(-50%);
}
.main-heading h2 span{
    font-size: 55px;
    display: block;
    text-transform: none;
    font-weight: 400;
    font-family: 'Kaushan Script', cursive;
    margin-bottom: 20px;
}
.main-heading .banner-btn{
    display: inline-block;
    color: #fff;
    text-transform: uppercase;
    padding: 10px 20px;
    text-align: center;
    border: 2px solid #fff;
    font-size: 12px;
    line-height: 100%;
}
.carousel-indicators{
    width: 100%;
    left: 0;
    margin: 0;
    position: static;
    margin-top: 80px;
    justify-content: space-between;
}
.carousel-indicators li.active, .carousel-indicators li{
    float: left;
    width: 25%;
    height: auto;
    margin: 0;
    text-indent:1px;
    border-radius: 0;
    background: none;
    text-align: left;
    border: none;
}
.carousel-indicators .inner{
    border-top: 2px solid #f7eac4;
    position: relative;
    color: #faf3e0;
    text-transform: uppercase;
    margin: 0 12px;
    padding: 10px 0;
}
.carousel-indicators li:active .inner,.carousel-indicators li:hover .inner{
    border-color: #fff;
	color: #fff;
}
.carousel-indicators li .number {
    font-weight: 700;
    font-size: 18px;
    margin-right: 3px;
}
.carousel-indicators li:hover .inner:after,
.carousel-indicators li.active .inner:after{
    content: "";
    position: absolute;
    left: 0;
    top: -2px;
    width: 60px;
    height: 2px;
    background: #f38181;
}
/* section */
section{
    padding: 80px 0px;
}
h1, h2, h3, h4, h5, h6 {
    color: #333;
    text-transform: uppercase;
    font-family: 'Montserrat', sans-serif;
    margin: 5px 0px 0px;
}
p{
    font-size: 15px;
    margin: 0;
    padding: 0;
    font-weight: 400;
    font-family: 'Roboto', sans-serif;
}
.sectionHeading{
    font-weight: 700;
    font-size: 30px;
    padding-bottom: 10px;
    margin-bottom: 20px;
    text-align: center;
    position: relative;
}
.sectionHeading::after{
    content: "";
    position: absolute;
    left: 50%;
    bottom: 0;
    width: 60px;
    height: 3px;
    background: #f38181;
    transform: translateX(-50%);
}
.content{
    text-align: center;
    max-width: 750px;
    margin: 0 auto 40px;
}
.subHeading{
    display: block;
    font-weight: 400;
    font-family: 'Kaushan Script', cursive;
    font-size: 24px;
    margin-bottom: 20px;
    text-transform: none;
}
/* .about-story{
    text-align: center;
} */
 .img-area{
    background-color: #95e1d3;
    display: inline-block;
}
 .img-area .overlay{
    display: block;
    position: relative;
    left: 0;
    top: 0;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    -ms-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}
 .img-area .overlay::before{
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    background: rgb(252,227,138);
    background: linear-gradient(0deg, rgba(252,227,138,0.9) 0%, rgba(243,129,129,0.9) 100%);
    background: -webkit-linear-gradient(0deg, rgba(252,227,138,0.9) 0%, rgba(243,129,129,0.9) 100%);
    background: -o-linear-gradient(0deg, rgba(252,227,138,0.9) 0%, rgba(243,129,129,0.9) 100%);
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    -ms-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
   
}
.img-area:hover .overlay{
    left: -10px;
    top:-10px;
}
.img-area:hover .overlay::before{
    opacity: 1;
}
.img-text{
    position: absolute;
    left: 0;
    top: 50%;
    width: 100%;
    font-weight: 700;
    font-size: 18px;
    z-index: 1;
    opacity: 0;
    text-align: center;
    color: #fff;
    text-transform: uppercase;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
}
 .img-text:hover .about-icon{
    opacity: 1;
  
}
/* .about-story:hover .img-text .about-icon{
    display: block;
    font-size: 30px;
} */
/* .img-text::before{
   
} */
 .img-area:hover .img-text{
    opacity: 1;
}
.img-area:hover .img-text::before{
    content: "";
    background-image: url(../img/about-team.png);
    background-position: center;
    z-index: 1;
    background-color: black;
}
/* counter section */
.counter{
    padding: 0;
	background: #95e1d3;
	font-family: 'Montserrat', sans-serif;
	text-align: center;
	color: #fff;
	text-transform: uppercase;
	font-size: 14px;
}
.counter-area{
    width: 20%;
	padding: 60px 5px;
	border-right: 1px solid rgba(255, 255, 255, 0.35);
}
.counter-area:first-child{
    border-left: 1px solid rgba(255, 255, 255, 0.35);
}
.counter-area .number{
    font-size: 72px;
}
.counter-area .text{
    font-size: 14px;
}